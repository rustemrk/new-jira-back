<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="updateTableTodoStatus" author="Rustem">
        <addColumn tableName="todo_status">
            <column name="create_date" type="timestamp with time zone"/>
        </addColumn>
    </changeSet>

    <changeSet id="updateTableTodoType" author="Rustem">
        <addColumn tableName="todo_type">
            <column name="create_date" type="timestamp with time zone"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>